{
  "cSpell.words": [
    "nosebit"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.run": "onSave",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
  ],
  "eslint.trace.server": "off",
  "eslint.lintTask.enable": false,
  "typescript.validate.enable": true
}
